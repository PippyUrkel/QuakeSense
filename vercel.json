{
    "version": 2,
    "builds": [
        {
            "src": "backend/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/vite.config.js",
            "use": "@vercel/static-build",
            "config": {
            "distDir": "backend/public"
            }
        }
        ],
        "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/server.js"
        }
        ]
    }